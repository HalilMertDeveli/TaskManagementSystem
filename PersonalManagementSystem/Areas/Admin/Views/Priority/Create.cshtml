@model PriorityCreateRequest
@{
    Layout = "_AdminLayout";
    ViewBag.Title = "Aciliyet Durum Ekle";
}

<form asp-action="Create" asp-controller="Priority" asp-area="Admin" class="mt-3" method="post">
    <div class="mb-3">
        <label  class="form-label">Aciliyet durumunu giriniz</label>
        <input type="text" class="form-control"  asp-for="Definition">
    </div>
   

    <div class="d-grid gap-2">
        <button type="submit" class="btn btn-primary">Kaydet</button>
    </div>

    <div asp-validation-summary="All" class="text-danger lead">

    </div>
</form>
